{
  "logger": {
    "level": "info",
    "frontends": [
      {
        "formatter": {
          "type": "string",
          "pattern": "%(timestamp)s %(request_id)s/%(lwp)s/%(pid)s %(severity)s: %(message)s %(...L)s"
        },
        "sink": {
          "type": "files",
          "path": "/dev/stdout",
          "autoflush": true,
          "rotation": {
            "move": 0
          }
        }
      }
    ]
  },
  "options": {
    "join": true,
    "flags": 4,
    "remote": [
      "{REMOTE_IP}:{REMOTE_PORT}:2"
    ],
    "address": [
      "{PUBLIC_IP}:{REMOTE_PORT}:2-2"
    ],
    "wait_timeout": 60,
    "check_timeout": 60,
    "io_thread_num": 2,
    "nonblocking_io_thread_num": 4,
    "net_thread_num": 2,
    "daemon": false,
    "parallel": true,
    "auth_cookie": "{AUTH_COOKIE}",
    "bg_ionice_class": 3,
    "bg_ionice_prio": 0,
    "server_net_prio": 1,
    "client_net_prio": 6,
    "cache": {
      "size": 68719476736
    },
    "indexes_shard_count": 2,
    "monitor": {
      "port": 0
    }
  },
  "backends": [
    {
      "backend_id": 1,
      "type": "blob",
      "group": 1,
      "history": "/var/lib/elliptics/history",
      "data": "/var/lib/elliptics/blobdata",
      "blob_flags": 1,
      "blob_size": "10G",
      "periodic_timeout": 15
    }
  ]
}